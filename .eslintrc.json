{
  "plugins": ["import"],
  "rules": {
    "import/no-unresolved": "error",
    "import/no-named-as-default": "off",
    "import/no-named-as-default-member": "off",

    // âœ… This rule enforces case-sensitive imports
    "import/no-unresolved": [
      "error",
      { "caseSensitive": true }
    ]
  }
}
